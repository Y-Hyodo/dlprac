    "import glob\n",
    "import os\n",
    " \n",
    "# 拡張子.txtのファイルを取得する\n",
    "path = '*.jpg'\n",
    "i = 1723\n",
    " \n",
    "# jpgファイルを取得する\n",
    "flist = glob.glob(path)\n",
    "# print('変更前')\n",
    "# print(flist)\n",
    " \n",
    "# ファイル名を一括で変更する\n",
    "for file in flist:\n",
    "  os.rename(file, './rename/' + str(i) + '.jpg')\n",
    "  i+=1"
